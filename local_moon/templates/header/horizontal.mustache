<header id="moon-header" class="{{header.class}}"{{header.headAttrs}}>
    <nav class="navbar navbar-expand shadow-none">
        {{> local_moon/header/includes/burger }}
        <div class="header-left-section as-gutter-x-xl@lg d-flex justify-content-start{{#header.is_left}} flex-{{header.header_breakpoint}}-grow-1{{/header.is_left}}">
            {{> local_moon/header/includes/logo }}
            {{#header.is_left}}
                {{> local_moon/header/includes/primary_menu }}
            {{/header.is_left}}
        </div>
        {{#header.is_center}}
            <div class="header-center-section d-none d-{{header.header_breakpoint}}-flex justify-content-center flex-{{header.header_breakpoint}}-grow-1">
                {{> local_moon/header/includes/primary_menu }}
            </div>
        {{/header.is_center}}
        <div class="header-right-section as-gutter-x-xl@lg d-none d-{{header.header_breakpoint}}-flex justify-content-end {{#header.is_right}}flex-{{header.header_breakpoint}}-grow-1{{/header.is_right}}">
            {{#header.is_right}}
                {{> local_moon/header/includes/primary_menu }}
            {{/header.is_right}}
            <ul class="navbar-nav d-none d-md-flex my-1 px-1">
                <!-- page_heading_menu -->
                {{{ output.page_heading_menu }}}
            </ul>
            <div id="usernavigation" class="navbar-nav h-100">
                {{# output.search_box }}
                    {{{ output.search_box }}}
                    <div class="divider border-start h-75 align-self-center mx-1"></div>
                {{/output.search_box}}
                {{#langmenu}}
                    {{> theme_boost/language_menu }}
                    <div class="divider border-start h-75 align-self-center mx-1"></div>
                {{/langmenu}}
                {{{ output.navbar_plugin_output }}}
                <div class="d-flex align-items-stretch usermenu-container" data-region="usermenu">
                    {{#usermenu}}
                        {{> core/user_menu }}
                    {{/usermenu}}
                </div>
                {{{ output.edit_switch }}}
            </div>
        </div>
    </nav>
</header>